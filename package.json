{
  "name": "botten-nappet",
  "version": "1.0.0",
  "description": "A Twitch bot and streaming tool",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node $npm_package_main",
    "debug": "node --inspect $npm_package_main",
    "debug:break": "node --inspect --inspect-brk $npm_package_main",
    "clean": "rimraf dist/",
    "build": "npm run --silent build:index",
    "build:index": "rollup --config rollup.config.index.js",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch": "npm run --silent watch:index",
    "watch:index": "rollup --watch --config rollup.config.index.js",
    "test": "npm run --silent lint:fast",
    "lint": "npm run --silent lint:full",
    "lint:fast": "npm run --silent eslint && npm run --silent lint:copyright",
    "lint:full": "npm run --silent lint:fast",
    "lint:copyright": "find . -not \\( -path './.git/*' -or -path './node_modules/*' -or -path './dist/*' \\) -type f \\( -iname '*.ts' \\) -print0 | xargs -0 grep -L 'This file is part of botten-nappet' | sed 's/^/File is missing copyright notice: /'",
    "eslint": "eslint --ext .ts .",
    "eslint:fix": "eslint --ext .ts --fix ."
  },
  "keywords": [
    "twitch",
    "bot",
    "streaming"
  ],
  "author": {
    "name": "Joel Purra",
    "email": "mig@joelpurra.se",
    "url": "https://joelpurra.com/"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpurra/botten-nappet.git"
  },
  "bugs": {
    "url": "https://joelpurra.com/support/"
  },
  "homepage": "https://joelpurra.com/projects/botten-nappet/",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "camo": "^0.12.3",
    "deep-strict-equal": "^0.2.0",
    "nedb": "^1.8.0",
    "pino": "^4.10.3",
    "qs": "^6.5.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.8.1",
    "power-assert": "^1.4.4",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-ejs": "^1.1.1",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^12.0.0",
    "uglify-es": "^3.3.7"
  }
}
