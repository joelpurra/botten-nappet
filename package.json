{
  "name": "botten-nappet",
  "version": "5.0.0",
  "description": "A Twitch bot and streaming tool",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node $npm_package_main",
    "debug": "node --inspect $npm_package_main",
    "debug:break": "node --inspect --inspect-brk $npm_package_main",
    "clean": "rimraf ./dist/ ./src/frontend/public/dist/",
    "build": "npm run --silent build:index && npm run --silent build:client && npm run --silent build:copy:client:libs",
    "build:index": "rollup --config rollup.config.index.js",
    "build:client": "rollup --config rollup.config.client.js",
    "build:copy:client:libs": "cat ./node_modules/paper/dist/paper-full.min.js ./node_modules/soundjs/lib/soundjs.min.js ./node_modules/animejs/anime.min.js ./node_modules/rxjs/bundles/Rx.min.js > ./src/frontend/public/dist/libs.js",
    "rebuild": "npm run --silent clean && npm run --silent build",
    "watch:index": "rollup --watch --config rollup.config.index.js",
    "watch:client": "rollup --watch --config rollup.config.client.js",
    "test": "npm run --silent lint:fast",
    "lint": "npm run --silent lint:full",
    "lint:fast": "npm run --silent tslint && npm run --silent lint:copyright",
    "lint:full": "npm run --silent lint:fast",
    "lint:copyright": "find . -not \\( -path './.git/*' -or -path './node_modules/*' -or -path './dist/*' -or -path './src/backend/lib/*' \\) -type f \\( -iname '*.ts' \\) -print0 | xargs -0 grep -L 'This file is part of botten-nappet' | sed 's/^/File is missing copyright notice: /'",
    "tslint": "tslint --config tslint.json --project .",
    "tslint:fix": "tslint --config tslint.json --project ."
  },
  "keywords": [
    "twitch",
    "bot",
    "streaming"
  ],
  "author": {
    "name": "Joel Purra",
    "email": "mig@joelpurra.se",
    "url": "https://joelpurra.com/"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpurra/botten-nappet.git"
  },
  "bugs": {
    "url": "https://joelpurra.com/support/"
  },
  "homepage": "https://joelpurra.com/projects/botten-nappet/",
  "dependencies": {
    "@types/animejs": "^2.0.0",
    "@types/config": "0.0.34",
    "@types/koa": "^2.0.44",
    "@types/koa-static": "^4.0.0",
    "@types/moment": "^2.13.0",
    "@types/pkg-dir": "^2.0.0",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/soundjs": "^0.6.27",
    "animejs": "^2.2.0",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "camo": "^0.12.3",
    "check-types": "^7.3.0",
    "config": "^1.29.4",
    "js-yaml": "^3.10.0",
    "koa": "^2.5.0",
    "koa-static": "^4.0.2",
    "moment": "^2.20.1",
    "nedb": "^1.8.0",
    "paper": "^0.11.5",
    "pino": "^4.10.3",
    "pkg-dir": "^2.0.0",
    "qs": "^6.5.1",
    "rxjs": "^6.0.0-alpha.3",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "soundjs": "^1.0.1",
    "then-read-json": "^1.0.3",
    "ws": "^4.0.0",
    "zeromq-ng": "^5.0.0-beta.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/camo": "^0.12.0",
    "@types/check-types": "^7.3.0",
    "@types/nedb": "^1.8.5",
    "@types/node": "^9.4.0",
    "@types/pino": "^4.7.1",
    "@types/qs": "^6.5.1",
    "@types/ws": "^4.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-ejs": "^1.1.1",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "uglify-es": "^3.3.7"
  }
}
